    <!-- Blood Enhancements/Replacements -->
    
    <HediffDef ParentName="CNHediffOther">
        <defName>CNFiltrationBloodEnhancement</defName>
        <label>Filtration Blood Enhancement</label>
        <labelNoun>Filtration Blood Enhancement</labelNoun>
        <comps>
            <li Class="CyberNet.HediffCompPropertiesCyberNetImplant">
                <compClass>CyberNet.HediffCompCyberNetImplant</compClass>
                <DefaultSeverity>1</DefaultSeverity>
                <BandwidthUsage>1</BandwidthUsage>
                <ProcessorUsage>0.5</ProcessorUsage>
            </li>
        </comps>
        <stages>
            <li>
                <label>disconnected</label>
                <minSeverity>0</minSeverity>
                <capMods>
                    <li>
                        <capacity>BloodFiltration</capacity>
                        <offset>-0.1</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>throttled</label>
                <minSeverity>0.5</minSeverity>
                <capMods>
                    <li>
                        <capacity>BloodFiltration</capacity>
                        <offset>0</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>sluggish</label>
                <minSeverity>0.75</minSeverity>
                <capMods>
                    <li>
                        <capacity>BloodFiltration</capacity>
                        <offset>0.1</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>connected</label>
                <minSeverity>1</minSeverity>
                <capMods>
                    <li>
                        <capacity>BloodFiltration</capacity>
                        <offset>0.2</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>overclocked</label>
                <minSeverity>1.15</minSeverity>
                <capMods>
                    <li>
                        <capacity>BloodFiltration</capacity>
                        <offset>0.225</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>supercharged</label>
                <minSeverity>1.3</minSeverity>
                <capMods>
                    <li>
                        <capacity>BloodFiltration</capacity>
                        <offset>0.25</offset>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>

