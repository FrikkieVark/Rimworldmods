    <defName>CNInstallAdvancedNetworkedArm</defName>
    <label>Install Advanced Networked Arm</label>
    <description>Install an Advanced Networked Arm</description>
    <jobString>Installing Advanced Networked Arm</jobString>
    <workAmount>3400</workAmount>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CNAdvancedNetworkedArm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>CNAdvancedNetworkedArm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Shoulder</li>
    </appliedOnFixedBodyParts>
    <addsHediff>CNAdvancedNetworkedArm</addsHediff>
  </RecipeDef>

