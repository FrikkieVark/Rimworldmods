  <!-- Networked (Bionic) Level Body Parts -->
  
  <RecipeDef ParentName="CyberNetSurgery">
    <defName>CNInstallNetworkedArm</defName>
    <label>Install Networked Arm</label>
    <description>Install a Networked Arm</description>
    <jobString>Installing Networked Arm</jobString>
    <workAmount>2500</workAmount>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CNNetworkedArm</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>CNNetworkedArm</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Shoulder</li>
    </appliedOnFixedBodyParts>
    <addsHediff>CNNetworkedArm</addsHediff>
  </RecipeDef>

