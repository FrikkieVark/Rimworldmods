<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    
    <!-- Networked (Bionic) Level Body Parts -->
    
    <HediffDef ParentName="CNHediffProsthetic">
        <defName>CNNetworkedArm</defName>
        <label>Networked Arm</label>
        <labelNoun>A Networked Arm</labelNoun>
		<description>A networked prosthetic that offloads processing to remote processors. Cheaper than self contained bionic arms, and more efficient, but requires infrastructure to function.</description>
        <spawnThingOnRemoved>CNNetworkedArm</spawnThingOnRemoved>
        <comps>
            <li Class="CyberNet.HediffCompPropertiesCyberNetImplant">
                <compClass>CyberNet.HediffCompCyberNetImplant</compClass>
                <DefaultSeverity>1</DefaultSeverity>
                <BandwidthUsage>0.5</BandwidthUsage>
                <ProcessorUsage>0.25</ProcessorUsage>
            </li>
            <li Class="HediffCompProperties_VerbGiver">
                <tools>
                    <li>
                        <label>fist</label>
                        <capacities>
                            <li>Blunt</li>
                        </capacities>
                        <power>12</power> <!-- Same as a bionic arm -->
                        <cooldownTime>2</cooldownTime>
                    </li>
                </tools>
            </li>
        </comps>
        <stages>
            <li>
                <label>disconnected</label>
                <minSeverity>0</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>-0.1</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>throttled</label>
                <minSeverity>0.5</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>-0.05</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>sluggish</label>
                <minSeverity>0.75</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>connected</label>
                <minSeverity>1</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.15</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>overclocked</label>
                <minSeverity>1.15</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.175</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>supercharged</label>
                <minSeverity>1.3</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.2</offset>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>

