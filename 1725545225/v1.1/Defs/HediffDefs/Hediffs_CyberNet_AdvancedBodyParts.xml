<Defs>

    <!-- Advanced Networked (Archotech) Level Body Parts -->
    
    <HediffDef ParentName="CNHediffProsthetic">
        <defName>CNAdvancedNetworkedArm</defName>
        <label>Advanced Networked Arm</label>
        <labelNoun>An Advanced Networked Arm</labelNoun>
		<description>An advanced networked prosthetic that offloads processing to remote processors. Cheaper than self contained bionic arms, and it's significantly more efficient. The cost comes from the infrastructure required for it to function.</description>
        <spawnThingOnRemoved>CNAdvancedNetworkedArm</spawnThingOnRemoved>
        <comps>
            <li Class="CyberNet.HediffCompPropertiesCyberNetImplant">
                <compClass>CyberNet.HediffCompCyberNetImplant</compClass>
                <DefaultSeverity>1</DefaultSeverity>
                <BandwidthUsage>1.5</BandwidthUsage>
                <ProcessorUsage>0.75</ProcessorUsage>
            </li>
            <li Class="HediffCompProperties_VerbGiver">
                <tools>
                    <li>
                        <label>fist</label>
                        <capacities>
                            <li>Blunt</li>
                        </capacities>
                        <power>14</power> <!-- Same as a archotech arm -->
                        <cooldownTime>2</cooldownTime>
                    </li>
                </tools>
            </li>
        </comps>
        <stages>
            <li>
                <label>disconnected</label>
                <minSeverity>0</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>throttled</label>
                <minSeverity>0.5</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.075</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>sluggish</label>
                <minSeverity>0.75</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.15</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>connected</label>
                <minSeverity>1</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.225</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>overclocked</label>
                <minSeverity>1.15</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.25</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <label>supercharged</label>
                <minSeverity>1.3</minSeverity>
                <capMods>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>0.275</offset>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>

